{
  "openapi": "3.0.1",
  "info": {
    "title": "TopNews",
    "version": "1.0"
  },
  "paths": {
    "/TopNews/list": {
      "get": {
        "tags": [
          "TopNews"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/TopNews/add": {
      "post": {
        "tags": [
          "TopNews"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TopNewsItem"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TopNewsItem"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TopNewsItem"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/TopNews/get": {
      "get": {
        "tags": [
          "TopNews"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/TopNews/delete": {
      "delete": {
        "tags": [
          "TopNews"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TopNewsItem": {
        "required": [
          "hackerNewsId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "hackerNewsId": {
            "type": "integer",
            "format": "int64"
          },
          "title": {
            "maxLength": 128,
            "minLength": 1,
            "type": "string",
            "nullable": true
          },
          "author": {
            "maxLength": 128,
            "type": "string",
            "nullable": true
          },
          "text": {
            "maxLength": 32767,
            "type": "string",
            "nullable": true
          },
          "url": {
            "maxLength": 2083,
            "type": "string",
            "nullable": true
          },
          "publicationDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    }
  }
